          "metricColumn": "metric_type",
          "rawQuery": true,
          "rawSql": "SELECT\n  'ðŸ¥ Health Status' as Category,\n  'Service health: ' || (SELECT score FROM optiinfra_metrics.application_metrics WHERE metric_type = 'health' ORDER BY timestamp DESC LIMIT 1) || '% - ' || CASE WHEN (SELECT score FROM optiinfra_metrics.application_metrics WHERE metric_type = 'health' ORDER BY timestamp DESC LIMIT 1) >= 95 THEN 'Excellent' WHEN (SELECT score FROM optiinfra_metrics.application_metrics WHERE metric_type = 'health' ORDER BY timestamp DESC LIMIT 1) >= 90 THEN 'Good' ELSE 'Needs attention' END as Status\nUNION ALL\nSELECT\n  'â­ Quality Assessment' as Category,\n  'Quality score: ' || (SELECT score FROM optiinfra_metrics.application_metrics WHERE metric_type = 'quality' ORDER BY timestamp DESC LIMIT 1) || ' - ' || CASE WHEN (SELECT score FROM optiinfra_metrics.application_metrics WHERE metric_type = 'quality' ORDER BY timestamp DESC LIMIT 1) >= 80 THEN 'High quality' WHEN (SELECT score FROM optiinfra_metrics.application_metrics WHERE metric_type = 'quality' ORDER BY timestamp DESC LIMIT 1) >= 50 THEN 'Fair quality' ELSE 'Poor quality' END as Status\nUNION ALL\nSELECT\n  'ðŸ“ˆ Performance Metrics' as Category,\n  'Request rate: ' || (SELECT score FROM optiinfra_metrics.application_metrics WHERE metric_type = 'request_rate' ORDER BY timestamp DESC LIMIT 1) || ' req/s, Response time: ' || (SELECT avg(score) FROM optiinfra_metrics.application_metrics WHERE metric_type = 'response_time' AND timestamp >= now() - INTERVAL 1 HOUR) || 'ms' as Status\nUNION ALL\nSELECT\n  'ðŸ”§ Service Status' as Category,\n  'Application: ' || (SELECT application_name FROM optiinfra_metrics.application_metrics ORDER BY timestamp DESC LIMIT 1) || ' - ' || CASE WHEN (SELECT score FROM optiinfra_metrics.application_metrics WHERE metric_type = 'health' ORDER BY timestamp DESC LIMIT 1) >= 90 THEN 'Running optimally' ELSE 'Performance issues detected' END as Status",
