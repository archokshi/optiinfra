          "metricColumn": "resource_type",
          "rawQuery": true,
          "rawSql": "SELECT\n  'üéØ Performance Status' as Category,\n  'GPU utilization: ' || (SELECT utilization FROM optiinfra_metrics.resource_metrics WHERE resource_type = 'gpu' ORDER BY timestamp DESC LIMIT 1) || '% - ' || CASE WHEN (SELECT utilization FROM optiinfra_metrics.resource_metrics WHERE resource_type = 'gpu' ORDER BY timestamp DESC LIMIT 1) > 50 THEN 'High load' ELSE 'Idle/Normal' END as Status\nUNION ALL\nSELECT\n  'üå°Ô∏è Thermal Status' as Category,\n  'GPU temperature: ' || (SELECT utilization FROM optiinfra_metrics.resource_metrics WHERE resource_type = 'gpu_temperature' ORDER BY timestamp DESC LIMIT 1) || '¬∞C - ' || CASE WHEN (SELECT utilization FROM optiinfra_metrics.resource_metrics WHERE resource_type = 'gpu_temperature' ORDER BY timestamp DESC LIMIT 1) < 70 THEN 'Optimal' WHEN (SELECT utilization FROM optiinfra_metrics.resource_metrics WHERE resource_type = 'gpu_temperature' ORDER BY timestamp DESC LIMIT 1) < 85 THEN 'Warm' ELSE 'Hot' END as Status\nUNION ALL\nSELECT\n  '‚ö° Throughput Analysis' as Category,\n  'Current throughput: ' || (SELECT metric_value FROM optiinfra_metrics.performance_metrics WHERE metric_name = 'throughput' ORDER BY timestamp DESC LIMIT 1) || ' tokens/second' as Status\nUNION ALL\nSELECT\n  'üî§ Token Generation' as Category,\n  'Total tokens: ' || (SELECT metric_value FROM optiinfra_metrics.performance_metrics WHERE metric_name = 'tokens_generated' ORDER BY timestamp DESC LIMIT 1) || ' tokens' as Status",
