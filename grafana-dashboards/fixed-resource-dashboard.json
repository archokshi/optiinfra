          "metricColumn": "resource_type",
          "rawQuery": true,
          "rawSql": "SELECT\n  'üå°Ô∏è Thermal Status' as Category,\n  'GPU temperature: ' || (SELECT utilization FROM optiinfra_metrics.resource_metrics WHERE resource_type = 'gpu_temperature' ORDER BY timestamp DESC LIMIT 1) || '¬∞C - ' || CASE WHEN (SELECT utilization FROM optiinfra_metrics.resource_metrics WHERE resource_type = 'gpu_temperature' ORDER BY timestamp DESC LIMIT 1) < 70 THEN 'Optimal' WHEN (SELECT utilization FROM optiinfra_metrics.resource_metrics WHERE resource_type = 'gpu_temperature' ORDER BY timestamp DESC LIMIT 1) < 85 THEN 'Warm' ELSE 'Hot - Monitor closely' END as Status\nUNION ALL\nSELECT\n  '‚ö° Power Efficiency' as Category,\n  'Current power draw: ' || (SELECT utilization FROM optiinfra_metrics.resource_metrics WHERE resource_type = 'gpu_power' ORDER BY timestamp DESC LIMIT 1) || 'W - ' || CASE WHEN (SELECT utilization FROM optiinfra_metrics.resource_metrics WHERE resource_type = 'gpu_power' ORDER BY timestamp DESC LIMIT 1) < 100 THEN 'Efficient' WHEN (SELECT utilization FROM optiinfra_metrics.resource_metrics WHERE resource_type = 'gpu_power' ORDER BY timestamp DESC LIMIT 1) < 200 THEN 'Moderate' ELSE 'High consumption' END as Status\nUNION ALL\nSELECT\n  'üíæ Memory Usage' as Category,\n  'GPU memory: ' || (SELECT utilization FROM optiinfra_metrics.resource_metrics WHERE resource_type = 'gpu_memory' ORDER BY timestamp DESC LIMIT 1) || '% used - ' || CASE WHEN (SELECT utilization FROM optiinfra_metrics.resource_metrics WHERE resource_type = 'gpu_memory' ORDER BY timestamp DESC LIMIT 1) < 70 THEN 'Available' WHEN (SELECT utilization FROM optiinfra_metrics.resource_metrics WHERE resource_type = 'gpu_memory' ORDER BY timestamp DESC LIMIT 1) < 90 THEN 'Moderate' ELSE 'High usage' END as Status\nUNION ALL\nSELECT\n  'üéØ Utilization Analysis' as Category,\n  'GPU utilization: ' || (SELECT utilization FROM optiinfra_metrics.resource_metrics WHERE resource_type = 'gpu' ORDER BY timestamp DESC LIMIT 1) || '% - ' || CASE WHEN (SELECT utilization FROM optiinfra_metrics.resource_metrics WHERE resource_type = 'gpu' ORDER BY timestamp DESC LIMIT 1) < 20 THEN 'Underutilized' WHEN (SELECT utilization FROM optiinfra_metrics.resource_metrics WHERE resource_type = 'gpu' ORDER BY timestamp DESC LIMIT 1) < 80 THEN 'Optimal' ELSE 'High utilization' END as Status",
